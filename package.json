{
  "name": "lab-nodejs",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "devstart": "nodemon -r esm app.js",
    "build": "npx babel app.js --out-file app-compiled.js ",
