    "dist": "^0.1.2",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lib": "^4.3.3",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.7",
    "morgan": "~1.9.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3",
